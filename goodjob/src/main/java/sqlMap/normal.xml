<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.goodjob.normalmember">
  	<insert id="normalJoin" parameterType="com.goodjob.normalmember.NormalMemberDTO">
  		insert into normalmember values(normalmember_idx.nextval,#{id},#{pwd},#{name},#{birth},#{gender},#{email},#{tel},#{addr},sysdate,0,'비활성')
  	</insert>
  	 <select id="comIdDuplicateCheck" parameterType="String" resultType="Integer">
  select count(*) from companymember where id=#{id} 
  </select>
  </mapper>